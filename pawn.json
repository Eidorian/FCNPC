{
    "user": "ziggi",
    "repo": "FCNPC",
    "include_path": "sampsvr_files",
    "dependencies": [
		"Pottus/ColAndreas",
		"Southclaws/samp-plugin-mapandreas"
    ],
    "resources": [
        {
            "name": "^FCNPC-(.+).zip$",
            "platform": "linux",
            "archive": true,
            "plugins": ["plugins/FCNPC.so"]
        },
        {
            "name": "^FCNPC-(.+).zip$",
            "platform": "windows",
            "archive": true,
            "plugins": ["plugins/FCNPC.dll"]
        }
    ],
    "runtime": {
        "plugins": ["ziggi/FCNPC"]
    }
}